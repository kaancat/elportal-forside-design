{
  "buildCommand": "npm run build:clean",
  "framework": null,
  "installCommand": "npm install --force",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/eloverblik.ts": {
      "maxDuration": 30
    },
    "api/electricity-prices.ts": {
      "maxDuration": 10
    },
    "api/co2-emissions.ts": {
      "maxDuration": 10
    },
    "api/energy-forecast.ts": {
      "maxDuration": 10
    },
    "api/monthly-production.ts": {
      "maxDuration": 10
    },
    "api/consumption-map.ts": {
      "maxDuration": 10
    },
    "api/track-click.ts": {
      "maxDuration": 5
    },
    "api/track-conversion.ts": {
      "maxDuration": 10
    },
    "api/track-pixel.ts": {
      "maxDuration": 3
    },
    "api/admin/auth.ts": {
      "maxDuration": 5
    },
    "api/admin/dashboard.ts": {
      "maxDuration": 15
    },
    "api/admin/debug.ts": {
      "maxDuration": 5
    }
  }
}