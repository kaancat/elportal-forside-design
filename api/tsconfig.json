{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": false,
    "outDir": ".vercel/output/functions"
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "dist"]
} 